%% Uppgift 1
f = @(x) x.^3 - cos(4*x);
x = linspace(-2, 1);

xHor = [-2 -1 0 1];
yHor = [0 0 0 0];

hold on
plot(xHor,yHor)
plot(x, f(x))
hold off

axis([-2 1 -1.5 1]), grid on

% Det finns 3st l?sningar
% x1 = -1
% x2 = -0.5
% x3 = 0.5

x1 = -1;
x2 = -0.5;

kmax=10;
tol=0.5e-8;
for k=1:kmax
    h=-f(x)/Df(x);
    x=x+h;
    disp([x h])
    
    if abs(h)<tol
        break
    end
end



